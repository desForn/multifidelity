FoamFile
{
	version		2.0;
	format		ascii;
	class		dictionary;
	location	"system";
	object		surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

inner.stl
{
	extractionMethod		extractFromSurface;

	extractFromSurfaceCoeffs
	{
		includedAngle		180;
	}

	writeObj			yes;
}

outer_cylinder.stl
{
	extractionMethod		extractFromSurface;

	extractFromSurfaceCoeffs
	{
		includedAngle		180;
	}

	writeObj			yes;
}

