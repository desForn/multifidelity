FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       surfaceScalarField;
    location    "0";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    ground
    {
        type            empty;
        value           nonuniform List<scalar> 0;
    }
    top
    {
        type            empty;
        value           nonuniform List<scalar> 0;
    }
    inner
    {
        type            calculated;
        value           uniform 0;
    }
    outer_cylinder
    {
        type            calculated;
        value           uniform 0;
    }
}


